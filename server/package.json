{
  "name": "safevideo-kyc-server",
  "version": "1.0.0",
  "description": "SafeVideo KYC (Know Your Customer) System API Server",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "node tests/scripts/run-all-tests.js",
    "test:unit": "jest --config tests/jest.config.js --testPathPattern=unit",
    "test:integration": "jest --config tests/jest.config.js --testPathPattern=integration --runInBand",
    "test:e2e": "cypress run --config-file tests/e2e/cypress.config.js",
    "test:e2e:open": "cypress open --config-file tests/e2e/cypress.config.js",
    "test:performance": "k6 run tests/performance/k6-config.js",
    "test:stress": "k6 run tests/performance/stress-test.js",
    "test:db-performance": "k6 run tests/performance/database-performance.js",
    "test:coverage": "jest --config tests/jest.config.js --coverage",
    "test:watch": "jest --config tests/jest.config.js --watch",
    "test:ci": "node tests/scripts/run-all-tests.js --skip-e2e",
    "lint": "eslint . --ext .js",
    "lint:fix": "eslint . --ext .js --fix",
    "format": "prettier --write \"**/*.{js,json,md}\"",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "db:reset": "npx sequelize-cli db:drop && npx sequelize-cli db:create && npm run migrate && npm run seed",
    "docs:generate": "swagger-jsdoc -d docs/swagger/definition.js routes/**/*.js -o docs/swagger/swagger.json",
    "docs:serve": "swagger-ui-serve docs/swagger/swagger.json",
    "build": "echo 'No build step required for Node.js backend'",
    "precommit": "npm run lint && npm run test:unit",
    "security:audit": "npm audit",
    "security:fix": "npm audit fix",
    "logs:clear": "rm -rf logs/*.log"
  },
  "keywords": [
    "kyc",
    "know-your-customer",
    "identity-verification",
    "compliance",
    "api",
    "nodejs",
    "express",
    "mysql",
    "sequelize"
  ],
  "author": "SafeVideo Team",
  "license": "MIT",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-rate-limit": "^6.11.2",
    "express-slow-down": "^1.6.0",
    "express-validator": "^6.15.0",
    "firebase-admin": "^11.8.0",
    "helmet": "^6.1.5",
    "ioredis": "^5.3.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.2.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.2.0",
    "nodemailer": "^6.9.1",
    "rate-limit-redis": "^3.1.0",
    "sequelize": "^6.30.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^4.7.1",
    "ws": "^8.13.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@faker-js/faker": "^8.4.1",
    "axe-core": "^4.7.0",
    "cypress": "^12.10.0",
    "cypress-axe": "^1.4.0",
    "cypress-file-upload": "^5.0.8",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "jest": "^29.5.0",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "supertest": "^6.3.3",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^4.6.2",
    "ts-loader": "^9.4.2",
    "webpack": "^5.82.1",
    "yamljs": "^0.3.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "jest": {
    "preset": "./tests/jest.config.js"
  }
}
